{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:prettier/recommended"
  ],
  "env": {
    "browser": true,
    "amd": true,
    "commonjs": true,
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": false,
    "codeFrame": false
  },
  "rules": {
    "semi": 1,
    "max-len": [1, {"code": 150}],
    "no-console": 0,
    "no-useless-escape": 0,
    "no-unused-expressions": [1, { "allowShortCircuit": false, "allowTernary": false }],
    "no-prototype-builtins": 0,
    "react/prop-types": [
      1,
      {
        "ignore": [
          "children",
          "location"
        ]
      }
    ],
    "react/no-unescaped-entities": 0,
    "quotes": [
      "error",
      "single"
    ],
    "react/display-name": 0
  },
  "globals": {
    "document": false,
    "console": false,
    "_frontendSocket": false,
    "_testSocket": false,
    "location": false,
    "_defaultSocket": false,
    "fetch": false,
    "Headers": false,
    "sessionStorage": false,
    "setTimeout": false
  },
  "settings": {
    "react": {
      "pragma": "React",
      "version": "16.0.0"
    }
  }
}